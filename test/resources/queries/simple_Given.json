{
	"title": "all columns are applykeys",
	"input": {
		"WHERE": {
			"AND": [{
				"IS": {
					"rooms_furniture": "Classroom-Fixed Tablets"
				}
			}, {
				"GT": {
					"rooms_seats": 300
				}
			}]
		},
		"OPTIONS": {
